<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@drawable/make_test_bg"
              android:orientation="vertical">

    <org.zywx.wbpalmstar.widgetone.uex11597450.weiget.overscroll.FastAndOverScrollScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/make_dark_container"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="@drawable/icon_make_test_top">

                <RelativeLayout
                    android:id="@+id/make_test_des_container"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/color_make_test">

                    <RelativeLayout
                        android:id="@+id/async_container"
                        style="@style/custome_title_bar_style"
                        android:background="@android:color/transparent">

                        <TextView
                            android:id="@+id/async_commit_local_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/border_space_sec_max"
                            android:gravity="center"
                            android:text="同步"
                            android:textColor="@color/color_white"/>

                        <ImageView
                            android:id="@+id/make_test_async_commit_local_db"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/border_space_half"
                            android:layout_toLeftOf="@id/async_commit_local_tv"
                            android:src="@drawable/incon_refresh_db"/>
                    </RelativeLayout>

                    <LinearLayout
                        android:id="@+id/db_hint_update_container"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/async_container"
                        android:background="@color/color_test_blurry"
                        android:orientation="horizontal"
                        android:visibility="gone"
                        android:paddingBottom="@dimen/border_space"
                        android:paddingTop="@dimen/border_space">

                        <TextView
                            android:id="@+id/update_content_des"
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_gravity="center"
                            android:layout_weight="1.1"
                            android:paddingLeft="@dimen/border_space"
                            android:gravity="center"
                            android:maxLines="1"
                            android:ellipsize="end"
                            android:textColor="@color/color_blue"
                            android:textSize="@dimen/text_size_ml"/>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/db_update_btn"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/single_contact_btn_selector"
                                    android:paddingBottom="@dimen/border_space_half"
                                    android:paddingLeft="@dimen/border_space_sec_max"
                                    android:paddingRight="@dimen/border_space_sec_max"
                                    android:paddingTop="@dimen/border_space_half"
                                    android:text="@string/str_update"
                                    android:textColor="@color/color_white"/>

                                <TextView
                                    android:id="@+id/db_uptade_cancel_btn"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/border_space_sec_max"
                                    android:background="@drawable/white_cancel_shape"
                                    android:paddingBottom="@dimen/border_space_half"
                                    android:paddingLeft="@dimen/border_space_sec_max"
                                    android:paddingRight="@dimen/border_space_sec_max"
                                    android:paddingTop="@dimen/border_space_half"
                                    android:text="@string/str_cancel"
                                    android:textColor="@color/color_thr_gray"/>
                            </LinearLayout>
                        </RelativeLayout>

                    </LinearLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:layout_marginLeft="@dimen/border_space_thr"
                        android:layout_marginRight="@dimen/border_space_thr">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:orientation="vertical">

                            <RelativeLayout style="@style/make_test_rl_style">

                                <TextView
                                    android:id="@+id/make_test_title_des"
                                    style="@style/make_test_txt_des_style"
                                    android:text="@string/str_make_test_title"/>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_toRightOf="@id/make_test_title_des"
                                    android:orientation="vertical">

                                    <View
                                        android:layout_width="1dip"
                                        android:layout_height="0dip"
                                        android:layout_weight="1"/>

                                    <View
                                        style="@style/com_line"
                                        android:background="@color/color_white"/>

                                    <View
                                        android:layout_width="1dip"
                                        android:layout_height="0dip"
                                        android:layout_gravity="right"
                                        android:layout_weight="1"
                                        android:background="@color/color_white"/>
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_toLeftOf="@id/make_test_title_des"
                                    android:orientation="vertical">

                                    <View
                                        android:layout_width="1dip"
                                        android:layout_height="0dip"
                                        android:layout_weight="1"/>

                                    <View
                                        style="@style/com_line"
                                        android:background="@color/color_white"/>

                                    <View
                                        android:layout_width="1dip"
                                        android:layout_height="0dip"
                                        android:layout_gravity="left"
                                        android:layout_weight="1"
                                        android:background="@color/color_white"/>
                                </LinearLayout>

                            </RelativeLayout>


                            <RelativeLayout style="@style/make_test_rl_style">

                                <TextView
                                    android:id="@+id/make_topic_number_des"
                                    style="@style/make_test_txt_des_style"/>

                                <View
                                    android:layout_width="1dip"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentLeft="true"
                                    android:background="@color/color_white"/>

                                <View
                                    android:layout_width="1dip"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentRight="true"
                                    android:background="@color/color_white"/>

                            </RelativeLayout>


                            <RelativeLayout style="@style/make_test_rl_style">

                                <TextView
                                    android:id="@+id/make_test_person_number"
                                    style="@style/make_test_txt_des_style"/>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_toRightOf="@id/make_test_person_number"
                                    android:orientation="vertical">

                                    <View
                                        android:layout_width="1dip"
                                        android:layout_height="0dip"
                                        android:layout_gravity="right"
                                        android:layout_weight="1"
                                        android:background="@color/color_white"/>

                                    <View
                                        style="@style/com_line"
                                        android:background="@color/color_white"/>

                                    <View
                                        android:layout_width="1dip"
                                        android:layout_height="0dip"
                                        android:layout_weight="1"/>

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_toLeftOf="@id/make_test_person_number"
                                    android:orientation="vertical">

                                    <View
                                        android:layout_width="1dip"
                                        android:layout_height="0dip"
                                        android:layout_gravity="left"
                                        android:layout_weight="1"
                                        android:background="@color/color_white"/>

                                    <View
                                        style="@style/com_line"
                                        android:background="@color/color_white"/>

                                    <View
                                        android:layout_width="1dip"
                                        android:layout_height="0dip"
                                        android:layout_weight="1"/>
                                </LinearLayout>

                            </RelativeLayout>

                        </LinearLayout>
                    </RelativeLayout>

                </RelativeLayout>
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/border_space_sec_max"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/pic_search_container"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <RelativeLayout
                            android:id="@+id/make_camera_container"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:background="@drawable/white_circle_bg">

                            <ImageView
                                android:layout_width="@dimen/border_space_thr"
                                android:layout_height="@dimen/border_space_thr"
                                android:layout_centerInParent="true"
                                android:src="@drawable/icon_camera"/>
                        </RelativeLayout>

                        <View
                            style="@style/com_line"
                            android:layout_centerVertical="true"
                            android:layout_toLeftOf="@id/make_camera_container"
                            android:background="@color/color_white"/>

                        <View
                            style="@style/com_line"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/make_camera_container"
                            android:background="@color/color_white"/>
                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/border_space"
                        android:text="@string/str_pic_search"
                        android:textColor="@color/color_white"
                        android:textSize="@dimen/text_size_micro_plus"/>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/language_search_container"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <RelativeLayout
                            android:id="@+id/make_language_container"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:background="@drawable/white_circle_bg">

                            <ImageView
                                android:layout_width="@dimen/border_space_thr"
                                android:layout_height="@dimen/border_space_thr"
                                android:layout_centerInParent="true"
                                android:src="@drawable/icon_language"/>
                        </RelativeLayout>

                        <View
                            style="@style/com_line"
                            android:layout_centerVertical="true"
                            android:layout_toLeftOf="@id/make_language_container"
                            android:background="@color/color_white"/>

                        <View
                            style="@style/com_line"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/make_language_container"
                            android:background="@color/color_white"/>
                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/border_space"
                        android:text="@string/str_language_search"
                        android:textColor="@color/color_white"
                        android:textSize="@dimen/text_size_micro_plus"/>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/find_search_container"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <RelativeLayout
                            android:id="@+id/make_search_container"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:background="@drawable/white_circle_bg">

                            <ImageView
                                android:layout_width="@dimen/border_space_thr"
                                android:layout_height="@dimen/border_space_thr"
                                android:layout_centerInParent="true"
                                android:src="@drawable/icon_search"/>
                        </RelativeLayout>

                        <View
                            style="@style/com_line"
                            android:layout_centerVertical="true"
                            android:layout_toLeftOf="@id/make_search_container"
                            android:background="@color/color_white"/>

                        <View
                            style="@style/com_line"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/make_search_container"
                            android:background="@color/color_white"/>
                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/border_space"
                        android:text="@string/str_insp_search"
                        android:textColor="@color/color_white"
                        android:textSize="@dimen/text_size_micro_plus"/>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/know_point_practice_container"
                        style="@style/make_test_frag_con_style">

                        <ImageView
                            style="@style/make_test_img_style"
                            android:src="@drawable/know_point_contact"/>

                        <TextView
                            style="@style/make_test_txt_style"
                            android:text="@string/str_know_point_practice"/>
                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/single_contact_container"
                        style="@style/make_test_frag_con_style">


                        <ImageView
                            style="@style/make_test_img_style"
                            android:src="@drawable/single_contact"/>

                        <TextView
                            style="@style/make_test_txt_style"
                            android:text="@string/str_single_contact"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/diff_practice_container"
                        style="@style/make_test_frag_con_style">


                        <ImageView
                            style="@style/make_test_img_style"
                            android:src="@drawable/difficulty_make"/>

                        <TextView
                            style="@style/make_test_txt_style"
                            android:text="@string/str_difficulty_make"/>
                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/make_test_intelligent_container"
                        style="@style/make_test_frag_con_style">

                        <ImageView
                            style="@style/make_test_img_style"
                            android:src="@drawable/intelligent_office"/>

                        <TextView
                            style="@style/make_test_txt_style"
                            android:text="@string/str_order_practice"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/simulation_test_container"
                        style="@style/make_test_frag_con_style">


                        <ImageView
                            style="@style/make_test_img_style"
                            android:src="@drawable/simulation_test"/>

                        <TextView
                            style="@style/make_test_txt_style"
                            android:text="@string/str_simulation_test"/>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/report_container"
                        style="@style/make_test_frag_con_style">


                        <ImageView
                            style="@style/make_test_img_style"
                            android:src="@drawable/gmat_report"/>

                        <TextView
                            style="@style/make_test_txt_style"
                            android:text="@string/str_gmat_report"/>

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

    </org.zywx.wbpalmstar.widgetone.uex11597450.weiget.overscroll.FastAndOverScrollScrollView>

</LinearLayout>